{
  "name": "active-circles-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:interview": "tsx src/interview/devRunMission.ts",
    "dev:interview:showcase": "tsx src/interview/devRunShowcase.ts",
    "dev:mission-worker": "tsx src/interview/missionWorkerRunner.ts",
    "dev:workers": "tsx watch workers/runner.ts",
    "dev:worker:stability": "WORKERS=stability tsx watch workers/runner.ts",
    "dev:worker:mission": "WORKERS=mission tsx watch workers/runner.ts",
    "dev:worker:cleanup": "WORKERS=cleanup tsx watch workers/runner.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "start:mission-worker": "node dist/interview/missionWorkerRunner.js",
    "start:workers": "node dist/workers/runner.js",
    "start:worker:stability": "WORKERS=stability node dist/workers/runner.js",
    "start:worker:mission": "WORKERS=mission node dist/workers/runner.js",
    "start:worker:cleanup": "WORKERS=cleanup node dist/workers/runner.js",
    "typecheck": "tsc -p tsconfig.test.json --noEmit",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.0",
    "@aws-sdk/client-bedrock-runtime": "^3.614.0",
    "@aws-sdk/client-ses": "^3.936.0",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.10.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^5.22.0",
    "ulid": "^3.0.1",
    "resend": "^6.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^7.0.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.1.4"
  }
}
